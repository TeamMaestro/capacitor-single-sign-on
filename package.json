{
    "name": "@teamhive/capacitor-single-signon",
    "version": "3.0.0",
    "description": "This plugin is designed to streamline the single signon process between android and ios",
    "main": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "build": "npm run clean && tsc",
        "clean": "rm -rf ./dist",
        "watch": "tsc --watch",
        "prepublishOnly": "npm run build"
    },
    "author": "TeamHive",
    "contributors": [
        "Osei Fortune",
        "John Pinkster",
        "Sean Bannigan"
    ],
    "license": "MIT",
    "dependencies": {
        "@capacitor/core": "latest"
    },
    "devDependencies": {
        "typescript": "^2.6.2"
    },
    "files": [
        "README.md",
        "dist/",
        "ios/",
        "android/",
        "TeamhiveCapacitorSingleSignon.podspec"
    ],
    "keywords": [
        "capacitor",
        "plugin",
        "native",
        "sso",
        "single",
        "signon"
    ],
    "capacitor": {
        "ios": {
            "src": "ios"
        },
        "android": {
            "src": "android"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TeamHive/capacitor-single-sign-on"
    },
    "bugs": {
        "url": "https://github.com/TeamHive/capacitor-single-sign-on/issues"
    }
}
